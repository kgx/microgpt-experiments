{
  "name": "alice_v2",
  "description": "Alice chunked prose with no trailing BOS (continuation across chunks) and more steps for coherence.",
  "data": {
    "path": "data/alice/alice.txt",
    "format": "chunked",
    "chunk_size": 128,
    "chunk_overlap": 64,
    "trailing_bos": false
  },
  "model": {"n_embd": 32, "n_head": 4, "n_layer": 2, "block_size": 128},
  "training": {"learning_rate": 0.01, "beta1": 0.9, "beta2": 0.95, "eps_adam": 1e-8, "num_steps": 5000}
}
